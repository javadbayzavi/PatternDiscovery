@model discovery.Models.scenarioviewmodel
@{

}
<div class="card">
    <div class="card-header">
        <h3 class="card-title">Dataset Details</h3>
        <a href="~/scenario"> <span class="fa fa-fast-backward fa-backward float-right"></span></a>
    </div>
    <!-- /.card-header -->
    <div class="card-body">
        <table id="datasettbl" class="table table-bordered table-hover table-striped">
            <thead>
                <tr>
                    <th>
                        ID
                    </th>
                    <td>
                        @Model.ID
                    </td>
                </tr>
                <tr>
                    <th>
                        Name
                    </th>
                    <td>
                        @Model.name
                    </td>
                </tr>
                <tr>
                    <th>
                        Version
                    </th>
                    <td>
                        @Model.sversion.ToString()
                    </td>
                </tr>
                <tr>
                    <th>
                        Date
                    </th>
                    <td>
                        @Model.createddate
                    </td>
                </tr>
                <tr>
                    <th>
                        Source
                    </th>
                    <td>
                        @Model.datasource
                    </td>
                </tr>
                <tr>
                    <th>
                        Source Type
                    </th>
                    <td>
                        @Model.sourcetype.ToString()
                    </td>
                </tr>
                <tr>
                    <th>
                        Analyze Method
                    </th>
                    <td>
                        @Model.method.ToString()
                    </td>
                </tr>
                <tr>
                    <th>
                        Status
                    </th>
                    <td>
                        @Model.status.ToString()
                    </td>
                </tr>

            </thead>
        </table>
    </div>
    <!-- /.card-body -->
</div>
